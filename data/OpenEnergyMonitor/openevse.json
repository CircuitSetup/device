{
    "name": "OpenEVSE",
    "category": "OpenEnergyMonitor",
    "group": "OpenEVSE",
    "description": "OpenEVSE auto configuration",
    "inputs": [
        {
            "name": "amp",
            "description": "Real-time charging current",
            "processList": [
                {
                    "process": "scale",
                    "arguments": { "type": "ProcessArg::VALUE", "value": 0.001 }
                },
                {
                    "process": "log_to_feed",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "current" }
                }
            ]
        },
        {
            "name": "wh",
            "description": "Cumulative Energy",
            "processList": [
                {
                    "process": "scale",
                    "arguments": { "type": "ProcessArg::VALUE", "value": 0.001 }
                },
                {
                    "process": "wh_accumulator",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "energy" }
                }
            ]
        },
        {
            "name": "temp1",
            "description": "EVSE internal temperature",
            "processList": [
                {
                    "process": "scale",
                    "arguments": { "type": "ProcessArg::VALUE", "value": 0.1 }
                },
                {
                    "process": "log_to_feed",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "temperature" }
                }
            ]
        },
        {
            "name": "state",
            "description": "State",
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "state" }
                }
            ]
        },
        {
            "name": "pilot",
            "description": "Pilot",
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "pilot" }
                }
            ]
        },
        {
            "name": "divertmode",
            "description": "Divert Mode state",
            "processList": [
                {
                    "process": "log_to_feed",
                    "arguments": { "type": "ProcessArg::FEEDID", "value": "divertmode_state" }
                }
            ]
        }
    ],

    "feeds": [
        {
            "name": "current",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": "A"
        },
        {
            "name": "energy",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": "kWh"
        },
        {
            "name": "temperature",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": "Â°C"
        },
        {
            "name": "state",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": ""
        },
        {
            "name": "pilot",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": "A"
        },
        {
            "name": "divertmode_state",
            "type": "DataType::REALTIME",
            "engine": "Engine::PHPFINA",
            "interval": "30",
            "unit": ""
        }
    ]
}
